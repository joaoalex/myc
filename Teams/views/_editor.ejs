<div class="form-group row">
  <label class="col-form-label col-sm-2" for="name">Nome</label>
  <div class="col-sm-8">
    <input autofocus class="form-control" name="name" maxlength="255" value="<%= model.name %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="description">Descrição</label>
  <div class="col-sm-7">
    <input class="form-control" name="description" maxlength="255" value="<%= model.description %>" />
  </div>
</div>


<div class="form-group row">
  <label class="col-form-label col-sm-2" for="segment">Segmento</label>
  <div class="col-sm-10">
    <textarea class="form-control" cols="20" name="segment" maxlength="30" rows="1"><%= model.segment %></textarea>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="metodologia">Metodologia</label>
  <div class="col-sm-10">
    <textarea class="form-control" cols="20" name="metodologia" maxlength="20" rows="1"><%= model.metodologia %></textarea>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="ativo">Ativo</label>
  <div class="col-sm-10">
    <textarea class="form-control" cols="20" name="ativo" maxlength="1" rows="1"><%= model.ativo %></textarea>
  </div>
</div>


